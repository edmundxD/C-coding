// Online C compiler to run C program online
#include <stdio.h>

int main(){
    int num,i;
    
    printf("Enter a positive integer: ");
    scanf("%d",&num);
    printf("The Multiplication table of %d is \n",num);
    
    for(i=1;i<=10;i++){
        printf("%d x %d= %d \n",num,i,num*i);
    }
    return 0;
}
    
