#include <stdio.h>

// Defining a structure called 'Person'
struct Person {
    char name[30];
    int age;
    float height;
};

int main() {
    // Initializing members of the structure
    struct Person person1 = {"Alice", 25, 5.7};

    // Displaying structure members
    printf("Name: %s\n", person1.name);
    printf("Age: %d\n", person1.age);
    printf("Height: %.1f\n", person1.height);

    return 0;
}
